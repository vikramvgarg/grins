# Mesh related options
[mesh-options]
mesh_option = read_mesh_from_file
mesh_filename = T_channel_quad_full.xda
element_type = QUAD9
make_mesh_second_order = true
uniformly_refine = 3

# Options for time solvers
[unsteady-solver]
transient = 'false' 
theta = 1.0
n_timesteps = 700
deltat = 50.0 

#Linear and nonlinear solver options
[linear-nonlinear-solver]
max_nonlinear_iterations = 100 
max_linear_iterations = 2500

verify_analytic_jacobians = 0.0

initial_linear_tolerance = 1.0e-10

use_numerical_jacobians_only = 'true'

require_residual_reduction = 'true'

# Visualization options
[vis-options]
output_vis = 'true'

vis_output_file_prefix = 'microfluidics' 

output_residual = 'false'

output_format = 'gmv'

#output_vars = 'rho'

# Options for print info to the screen
[screen-options]

system_name = 'ReducedElectroosmosis'

print_equation_system_info = true
print_mesh_info = true
print_log_info = true
solver_verbose = true
solver_quiet = false


print_qoi = 'false'
echo_qoi = 'false'

print_element_jacobians = 'false'

[../VariableNames]

Temperature = 'T'
u_velocity = 'u'
v_velocity = 'v'
w_velocity = 'w'
pressure = 'p'

[]
