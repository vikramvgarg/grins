# Options related to all Physics
[Physics]

enabled_physics = 'ReducedElectroosmosis'

# Options for  physics
[./ReducedElectroosmosis]

V_FE_family = 'LAGRANGE'
P_FE_family = 'LAGRANGE'
T_FE_family = 'LAGRANGE'

V_order = 'SECOND'
T_order = 'SECOND'
P_order = 'FIRST'

lambda = 1.0

