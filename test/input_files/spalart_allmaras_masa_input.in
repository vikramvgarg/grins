# Mesh related options
[Mesh]
   [./Generation]
      dimension = '2'
      x_min = '0.0'
      x_max = '1.0'
      y_min = '0.0'
      y_max = '1.0'
      n_elems_x = '128'
      n_elems_y = '128'
      element_type = 'QUAD9'

[]

# Options for time solvers
[unsteady-solver]
transient = 'true'
theta = '1.0'
n_timesteps = '1'
deltat = '0.1'

#Linear and nonlinear solver options
[linear-nonlinear-solver]
max_nonlinear_iterations = 25
max_linear_iterations = 10000

initial_linear_tolerance = 1.0e-12

#verify_analytic_jacobians = 1.e-6
use_numerical_jacobians_only = 'true'
[]

# Visualization options
[vis-options]
output_vis = 'true'
vis_output_file_prefix = 'sa_masa_free_shear'
output_format = 'ExodusII'

[Materials]

[./Viscosity]

mu = '2.0'

[]

# Options for print info to the screen
[screen-options]
print_equation_system_info = 'true'
print_mesh_info = 'true'
print_log_info = 'true'
solver_verbose = 'true'
solver_quiet = 'false'

echo_physics = 'true'

# Options related to all Physics
[Physics]

#enabled_physics = 'IncompressibleNavierStokes IncompressibleNavierStokesSPGSMStabilization SpalartAllmaras SpalartAllmarasSPGSMStabilization MasaSourceTerm'
enabled_physics = 'IncompressibleNavierStokes SpalartAllmaras MasaSourceTerm'

# Options for Incompressible Navier-Stokes physics
[./IncompressibleNavierStokes]

FE_family = LAGRANGE
V_order = SECOND
P_order = FIRST

viscosity_model = 'spalartallmaras'
#rho = 1.0

pin_pressure = true
#Pin value based on MASA source parameters below
pin_value = 1.0
pin_location = '0.0 0.0'

[../VariableNames]

u_velocity = 'u'
v_velocity = 'v'
pressure = 'p'
turbulent_viscosity = 'nu'

# Options for Spalart Allmaras physics
[../SpalartAllmaras]

TU_order = FIRST

#rho = 1.0

#no_of_walls = 2
#wall_ids = '0 2'

#ic_ids = '0'
#ic_types = 'parsed'
#ic_variables = 'nu'
#ic_values = '0.2+(0.1*cos((2.0*3.14*x)/1.0))+(0.2*cos((0.5*3.14*y)/1.0))'

infinite_distance = true

[./Parameters]
c_t3 = 0.0

# Options for MasaSourceTerm physics
[../../MasaSourceTerm]

solution_name = 'fans_sa_transient_free_shear'

mu = 2.0
u_0 = 1.0
u_x = 0.0
u_y = 0.1
v_0 = 0.0
v_x = 0.1
v_y = 0.0
rho_x = 0.0
rho_y = 0.0
# Dont forget to the pin the pressure using p_0, p_x and p_y
p_0 = 1.0
p_x = 0.0
p_y = 0.0
nu_sa_0 = 0.1
nu_sa_x = 0.1
nu_sa_y = 0.1

[./Variables]
names = 'u'
FE_types = 'LAGRANGE'
FE_orders = 'SECOND'


[]
